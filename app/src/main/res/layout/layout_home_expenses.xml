<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraint_expenses"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="0dp"
    android:background="@color/navbar_bg2">

    <ImageView
        android:id="@+id/background"
        android:layout_width="566dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@+id/view12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view13"
        app:srcCompat="@drawable/background_expenses" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_expense"
        android:layout_width="393dp"
        android:layout_height="488dp"
        android:layout_margin="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@color/colorBackground"
        android:clipToPadding="true"
        android:paddingTop="7dp"
        android:paddingBottom="7dp"
        app:layout_constraintBottom_toTopOf="@+id/view12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.748">

    </android.support.v7.widget.RecyclerView>

    <View
        android:id="@+id/view12"
        android:layout_width="wrap_content"
        android:layout_height="76dp"
        android:layout_margin="0dp"
        android:alpha="1"
        android:background="@color/expense_add_button2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/view13"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_margin="0dp"
        android:alpha="1"
        android:background="@color/expense_add_button2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/home"
        android:layout_width="51dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/bg_intro_transparent"
        android:scaleType="fitStart"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view12"
        app:srcCompat="@drawable/expense_home" />

    <LinearLayout
        android:id="@+id/view_expense"
        android:layout_width="395dp"
        android:layout_height="370dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="8dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view13"/>

    <ImageButton
        android:id="@+id/home_expenses"
        android:layout_width="51dp"
        android:layout_height="50dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/expenses_icon_clicked"
        android:scaleType="fitStart"
        app:layout_constraintStart_toEndOf="@+id/home"
        app:layout_constraintTop_toTopOf="@+id/view12"
        app:srcCompat="@drawable/expenses_icon_clicked" />

    <ImageButton
        android:id="@+id/home_accounts"
        android:layout_width="51dp"
        android:layout_height="50dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/bg_intro_transparent"
        android:scaleType="fitStart"
        app:layout_constraintStart_toEndOf="@+id/home_expenses"
        app:layout_constraintTop_toTopOf="@+id/view12"
        app:srcCompat="@drawable/account_icon_not_clicked" />

    <ImageButton
        android:id="@+id/expenses_budget"
        android:layout_width="51dp"
        android:layout_height="50dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="45dp"
        android:background="@drawable/bg_intro_transparent"
        android:scaleType="fitStart"
        app:layout_constraintEnd_toEndOf="@+id/view12"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/home_accounts"
        app:layout_constraintTop_toTopOf="@+id/view12"
        app:srcCompat="@drawable/budget_icon_not_clicked" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="42dp"
        android:layout_height="15dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Home"
        android:textColor="@color/expense_home_text544"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/view12"
        app:layout_constraintStart_toStartOf="@+id/view12"
        app:layout_constraintTop_toBottomOf="@+id/home"
        app:layout_constraintVertical_bias="0.777" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="59dp"
        android:layout_height="15dp"
        android:layout_marginStart="129dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Expenses"
        android:textColor="@color/expense_home_text544"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/view12"
        app:layout_constraintStart_toStartOf="@+id/view12"
        app:layout_constraintTop_toBottomOf="@+id/home_expenses"
        app:layout_constraintVertical_bias="0.777" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="59dp"
        android:layout_height="15dp"
        android:layout_marginStart="224dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Accounts"
        android:textColor="@color/expense_home_text544"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/view12"
        app:layout_constraintStart_toStartOf="@+id/view12"
        app:layout_constraintTop_toBottomOf="@+id/home"
        app:layout_constraintVertical_bias="0.777" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="59dp"
        android:layout_height="15dp"
        android:layout_marginStart="324dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Budgets"
        android:textColor="@color/expense_home_text544"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/view12"
        app:layout_constraintStart_toStartOf="@+id/view12"
        app:layout_constraintTop_toBottomOf="@+id/home"
        app:layout_constraintVertical_bias="0.777" />

    <Button
        android:id="@+id/expenses_create"
        android:layout_width="95dp"
        android:layout_height="37dp"
        android:layout_margin="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="36dp"
        android:background="@drawable/button_style"
        android:backgroundTint="@color/expense_add_button2_darker"
        android:text="Add"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toTopOf="@+id/view12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.973"
        app:layout_constraintStart_toStartOf="@+id/view13"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.013" />

</android.support.constraint.ConstraintLayout>