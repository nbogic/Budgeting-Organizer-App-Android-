        android:textColor="@color/colorAccent"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/view12"
        app:layout_constraintStart_toStartOf="@+id/view12"
        app:layout_constraintTop_toBottomOf="@+id/home"
        app:layout_constraintVertical_bias="0.777" />

    <Button
        android:id="@+id/home_profile_button"
        android:layout_width="96dp"
        android:layout_height="27dp"
        android:layout_margin="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="36dp"
        android:background="@color/expense_add_button_dark"
        android:text="Profile"
        android:textColor="@android:color/background_dark"
        android:textSize="10sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/view12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.973"
        app:layout_constraintStart_toStartOf="@+id/view13"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.013" />

    <TextView
        android:id="@+id/home_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="Text here"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toBottomOf="@+id/view13"
        app:layout_constraintEnd_toStartOf="@+id/home_profile_button"
        app:layout_constraintHorizontal_bias="0.034"
        app:layout_constraintStart_toStartOf="@+id/view13"
        app:layout_constraintTop_toTopOf="@+id/view13"
        app:layout_constraintVertical_bias="0.468" />

    <ImageButton
        android:id="@+id/home_accounts"
        android:layout_width="51dp"
        android:layout_height="50dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/bg_intro_transparent"
        android:onClick="onClick"
        android:scaleType="fitStart"
        app:layout_constraintStart_toEndOf="@+id/home_expenses"
        app:layout_constraintTop_toTopOf="@+id/view12"
        app:srcCompat="@drawable/account_icon_not_clicked" />

    <ImageButton
        android:id="@+id/home_expenses"
        android:layout_width="51dp"
        android:layout_height="50dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/bg_intro_transparent"
        android:onClick="onClick"
        android:scaleType="fitStart"
        app:layout_constraintStart_toEndOf="@+id/home"
        app:layout_constraintTop_toTopOf="@+id/view12"
        app:srcCompat="@drawable/expenses_icon_not_clicked" />

    <ImageButton
        android:id="@+id/home"
        android:layout_width="51dp"
        android:layout_height="50dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/bg_intro_transparent"
        android:scaleType="fitStart"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view12"
        app:srcCompat="@drawable/icon_home_clicked" />

    <ImageButton
        android:id="@+id/home_budget"
        android:layout_width="51dp"
        android:layout_height="50dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/bg_intro_transparent"
        android:onClick="onClick"
        android:scaleType="fitStart"
        app:layout_constraintStart_toEndOf="@+id/home_accounts"
        app:layout_constraintTop_toTopOf="@+id/view12"
        app:srcCompat="@drawable/budget_icon_not_clicked" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginBottom="8dp"
        android:text="Expenses"
        android:textColor="@color/expense_home_text"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView11"
