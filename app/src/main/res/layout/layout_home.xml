<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="0dp"
    android:background="@drawable/background_home4">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="440dp"
        android:layout_height="157dp"
        android:radius="70dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:srcCompat="@drawable/home_accounts_card_bg2" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="440dp"
        android:layout_height="157dp"
        android:radius="70dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view13"
        app:srcCompat="@drawable/home_expenses_card_bg3" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="440dp"
        android:layout_height="157dp"
        android:layout_marginBottom="30dp"
        android:radius="70dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/home_expenses"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.517"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/home_budget_card_bg2" />

    <View
        android:id="@+id/view12"
        android:layout_width="wrap_content"
        android:layout_height="76dp"
        android:layout_margin="0dp"
        android:alpha="1"
        android:background="@color/navbar_bg2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/view13"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:layout_margin="0dp"
        android:alpha="0.9"
        android:background="@color/expense_add_button2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.001" />

    <View
        android:id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_margin="0dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:alpha="1"
        android:background="@color/expense_add_button5"
        app:layout_constraintBottom_toTopOf="@+id/view12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.983" />

    <TextView
