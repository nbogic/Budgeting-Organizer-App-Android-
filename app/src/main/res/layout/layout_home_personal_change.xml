<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_intro_temporary" >

    <TextView
        android:id="@+id/home_change_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="12dp"
        android:text="Change _"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toTopOf="@+id/view32"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.109"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view6"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/view32"
        android:layout_width="359dp"
        android:layout_height="148dp"
        android:background="@color/colorBackground"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="181dp" />

    <TextView
        android:id="@+id/home_change_current"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter current _:"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="200dp" />

    <TextView
        android:id="@+id/home_change_new"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enter new _:"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="248dp" />

    <TextView
        android:id="@+id/home_change_new2"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        android:text="Enter again:"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="305dp" />

    <Button
        android:id="@+id/personal_confirm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CONFIRM"
        tools:layout_editor_absoluteX="281dp"
        tools:layout_editor_absoluteY="200dp" />

    <Button
        android:id="@+id/personal_cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CANCEL"
        tools:layout_editor_absoluteX="281dp"
        tools:layout_editor_absoluteY="265dp" />

    <EditText
        android:id="@+id/edit_current"
        android:layout_width="106dp"
        android:layout_height="31dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        android:textSize="10sp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="219dp" />

    <EditText
        android:id="@+id/edit_new"
        android:layout_width="106dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        android:textSize="10sp"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="263dp" />

    <EditText
        android:id="@+id/edit_new2"
        android:layout_width="106dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        android:textSize="10sp"
        tools:layout_editor_absoluteX="152dp"
        tools:layout_editor_absoluteY="263dp" />
</android.support.constraint.ConstraintLayout>